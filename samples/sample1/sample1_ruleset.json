{
    "name": "Ruleset 1",
    "scope": "event",
    "rule_set": [
        {
            "name": "weekday_only",
            "id": 1,
            "rules": [
                {
                    "type": "boundary",
                    "boundary": "period",
                    "period": ["mon", "tue", "wed", "thu", "fri"]
                }
            ]
